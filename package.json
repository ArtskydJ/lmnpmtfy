{
  "name": "lmnpmtfy",
  "description": "let me npm that for you",
  "version": "0.0.1",
  "repository": {
    "url": "https://github.com/ArtskydJ/lmnpmtfy.git"
  },
  "main": "js/index.js",
  "scripts": {
    "prestart": "browserify . -o ./bundle.js",
    "start": "nohup ecstatic -p 1026 &",
    "stop": "kill $(ps x | awk '/ecstatic [-]p 1026/ {print $1}') || echo ''",
    "running": "ps x | awk '/ecstatic [-]p 1026/ {print}'",
    "watch": "watchify . -o ./bundle.js"
  },
  "private": true,
  "dependencies": {
    "range-interval": "0.0.1"
  },
  "devDependencies": {}
}
